  const triggerWealthVibration = (amount) => {
    if (navigator.vibrate) {
      // Pulso discreto pero firme
      navigator.vibrate([200, 100, 200]);
    }
    setVibrating(true);
    addNotification(`Materialización de $${amount} detectada en cuenta.`);
    setTimeout(() => setVibrating(false), 3000);
