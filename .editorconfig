        {/* Métrica de Viralización */}
        <div className="grid grid-cols-1 md:grid-cols-4 gap-4">
          <StatCard icon={<Eye className="text-cyan-400" />} label="Visualizaciones" value={stats.views.toLocaleString()} sub="+12% esta hora" />
          <StatCard icon={<Users className="text-indigo-400" />} label="Seguidores" value={stats.followers.toLocaleString()} sub="Crecimiento Orgánico Forzado" />
          <StatCard icon={<DollarSign className="text-green-400" />} label="Ganancias (USD)" value={`$${stats.earnings.toLocaleString()}`} sub="Liquidez Fluida" />
